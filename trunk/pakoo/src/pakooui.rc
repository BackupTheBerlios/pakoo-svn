<!DOCTYPE kpartgui SYSTEM "kpartgui.dtd">
<kpartgui name="pakoo" version="2">
<MenuBar>
	<Menu name="file"><!--<text>&amp;File</text>-->
		<Action name="installebuild" />
		<Separator/>
	</Menu>
	<Menu name="repository"><text>&amp;Repository</text>
		<Action name="sync" />
		<Action name="update" />
		<Action name="cleanup" />
	</Menu>
	<Menu name="package"><text>&amp;Package</text>
		<Action name="install" />
		<Action name="uninstall" />
		<Separator/>
		<Action name="find" />
	</Menu>
</MenuBar>
<ToolBar fullWidth="true" name="mainToolBar" iconText="icontextbottom">
	<Action name="sync"/>
	<Action name="update"/>
	<Separator/>
	<Action name="install"/>
	<Action name="uninstall"/>
	<Separator/>
	<Action name="cleanup"/>
	<Action name="find" />
</ToolBar>
</kpartgui>
